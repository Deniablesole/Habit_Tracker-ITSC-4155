{
  "name": "habit-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "powershell -ExecutionPolicy Bypass -File ./scripts/setup.ps1",
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:web\"",
    "dev:web": "cd habit-web && vite --host 127.0.0.1 --port 5174",
    "dev:api": "cd backend && .venv\\Scripts\\python -m uvicorn app:app --reload --port 8000",
    "build:web": "cd habit-web && npm run build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "concurrently": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
